<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="visualization.ChangePassword">
   <children>
      <GridPane layoutX="78.0" layoutY="58.0" prefHeight="253.0" prefWidth="446.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="211.0" minWidth="10.0" prefWidth="180.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="285.0" minWidth="10.0" prefWidth="266.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="63.0" minHeight="10.0" prefHeight="63.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="63.0" minHeight="10.0" prefHeight="63.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Old Password" />
            <Label text="New Password" GridPane.rowIndex="1" />
            <Label text="Confirm New Password" GridPane.rowIndex="2" />
            <PasswordField fx:id="oldPasswordInput" promptText="Please type in your old password" GridPane.columnIndex="1" />
            <PasswordField fx:id="newPasswordInput" promptText="Please type in your new password" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <PasswordField fx:id="newPasswordAgainInput" promptText="Please type in your new password again" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Pane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="3">
               <children>
                  <Button layoutY="32.0" mnemonicParsing="false" onAction="#confirmOnClick" text="Confirm" />
                  <Button layoutX="207.0" layoutY="32.0" mnemonicParsing="false" onAction="#backOnClick" text="Back" />
               </children>
            </Pane>
         </children>
      </GridPane>
   </children>
</AnchorPane>
