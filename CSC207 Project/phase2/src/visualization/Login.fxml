<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="visualization.LoginClass">
   <children>
      <GridPane layoutX="120.0" layoutY="111.0" prefHeight="207.0" prefWidth="372.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="253.0" minWidth="10.0" prefWidth="190.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="330.0" minWidth="10.0" prefWidth="283.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints maxHeight="106.0" minHeight="10.0" prefHeight="95.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="106.0" minHeight="10.0" prefHeight="95.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="128.0" minHeight="10.0" prefHeight="102.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="136.0" minHeight="10.0" prefHeight="124.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Password" GridPane.rowIndex="2" />
            <Label text="Username" GridPane.rowIndex="1" />
            <TextField fx:id="usernameInput" prefHeight="23.0" prefWidth="157.0" promptText="Please type in your username" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <font>
                  <Font size="12.0" />
               </font></TextField>
            <Button mnemonicParsing="false" onAction="#loginButtonOnClick" prefHeight="23.0" prefWidth="81.0" text="Log in" GridPane.columnIndex="1" GridPane.rowIndex="3">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <PasswordField fx:id="passwordInput" prefHeight="27.0" prefWidth="250.0" promptText="Please type in your password" GridPane.columnIndex="1" GridPane.rowIndex="2">
               <font>
                  <Font size="12.0" />
               </font></PasswordField>
            <Pane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1">
               <children>
                  <TextFlow fx:id="dateDisplay" layoutX="23.0" layoutY="3.0" prefHeight="23.0" prefWidth="210.0" />
               </children>
            </Pane>
         </children>
      </GridPane>
      <Label layoutX="238.0" layoutY="72.0" prefHeight="19.0" prefWidth="136.0" text="Welcome!" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
